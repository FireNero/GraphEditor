<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryMousePressedColor" x:Shared="False" Color="#191919" />
	<SolidColorBrush x:Key="PrimaryMouseToggledColor" x:Shared="False" Color="CornflowerBlue" />
	<SolidColorBrush x:Key="PrimaryMouseOverColor" x:Shared="False" Color="#262626" />
	
	<SolidColorBrush x:Key="PrimaryPanelColor" x:Shared="False" Color="#FF2D2D30" />
    <SolidColorBrush x:Key="PrimaryPanelBorderColor" x:Shared="False" Color="#747474" />
    <SolidColorBrush x:Key="PrimaryHeaderPanelColor" x:Shared="False" Color="#4D4D4D" />

    <SolidColorBrush x:Key="PrimaryHeaderButtonColor" x:Shared="False" Color="#4D4D4D" />
    <SolidColorBrush x:Key="PrimaryButtonColor" x:Shared="False" Color="#3F3F3F" />
    <SolidColorBrush x:Key="PrimaryAlgoButtonColor" x:Shared="False" Color="#343434" />
	<SolidColorBrush x:Key="PrimaryContentColor" x:Shared="False" Color="White" />

	<SolidColorBrush x:Key="PrimaryCanvasColor" x:Shared="False" Color="#1E1E1E" />


</ResourceDictionary>