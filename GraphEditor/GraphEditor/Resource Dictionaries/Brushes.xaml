<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<SolidColorBrush x:Key="PrimaryMousePressedColor" x:Shared="False" Color="CornflowerBlue" />
	<SolidColorBrush x:Key="PrimaryMouseOverColor" x:Shared="False" Color="LightGray" />
	<SolidColorBrush x:Key="PrimaryPanelColor" x:Shared="False" Color="DimGray" />
	<SolidColorBrush x:Key="PrimaryButtonColor" x:Shared="False" Color="DimGray" />
	<SolidColorBrush x:Key="PrimaryContentColor" x:Shared="False" Color="White" />

</ResourceDictionary>